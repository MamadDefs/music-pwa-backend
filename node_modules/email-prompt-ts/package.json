{
  "name": "email-prompt-ts",
  "version": "0.3.2",
  "repository": "zeit/email-prompt",
  "license": "MIT",
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "description": "CLI email prompt with autocompletion and built-in validation",
  "scripts": {
    "build": "buble -y dangerousForOf index.js > build.js",
    "prepublish": "npm run build",
    "test": "xo"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "rules": {
      "no-use-before-define": 0,
      "func-names": 0
    }
  },
  "dependencies": {
    "ansi-escapes": "2.0.0",
    "chalk": "1.1.3"
  },
  "devDependencies": {
    "buble": "0.15.2",
    "xo": "0.18.2"
  }
}
